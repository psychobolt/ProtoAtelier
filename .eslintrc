{
    "parser": "babel-eslint",
    "extends": ["airbnb-base", "plugin:flowtype/recommended"],
    "plugins": ["flowtype", "jest"],
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-confusing-arrow": ["error", {"allowParens": true}],
        "flowtype/no-types-missing-file-annotation": 0
    },
    "env": {
        "jest/globals": true,
        "browser": true
    }
}